  <log4net>
    <root>
      <level value="ALL"/>
      <appender-ref ref="RollingFileAppender"/>
      <!--<appender-ref ref="sqlFullException" />-->
      <appender-ref ref="sqlNormal"/>
      <appender-ref ref="sqlNormal2"/>
    </root>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="D:/logs_Comex_WebSite/STP/api_inyector/[%date{yyyyMMdd}]_bch.api_inyector.log"/>
      <appendToFile value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="9"/>
      <maximumFileSize value="51100KB"/>
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger - %message%newline"/>
      </layout>
    </appender>
    <appender name="sqlNormal" type="log4net.Appender.AdoNetAppender">
      <bufferSize value="1"/>
      <connectionType value="System.Data.SqlClient.SqlConnection, System.Data, Version=1.0.3300.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"/>
      <connectionStringName value="CSLog4Net"/>
      <commandText value="INSERT INTO dbo.inyector_log (fch_carga, nmr_hilo, dsc_nivel, dsc_logger, dsc_mensaje, dsc_excepcion, nmb_metodo, dsc_json_request, canal, cdg_referencia)         VALUES (@fch_carga, @nmr_hilo, @dsc_nivel, @dsc_logger, @dsc_mensaje, @dsc_excepcion, @nmb_metodo, @dsc_json_request, @canal, @cdg_referencia)"/>
      <parameter>
        <parameterName value="@fch_carga"/>
        <dbType value="DateTime"/>
        <layout type="log4net.Layout.RawTimeStampLayout"/>
      </parameter>
      <parameter>
        <parameterName value="@nmr_hilo"/>
        <dbType value="String"/>
        <size value="255"/>
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%thread"/>
        </layout>
      </parameter>
      <parameter>
        <parameterName value="@dsc_nivel"/>
        <dbType value="String"/>
        <size value="50"/>
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%level"/>
        </layout>
      </parameter>
      <parameter>
        <parameterName value="@dsc_logger"/>
        <dbType value="String"/>
        <size value="255"/>
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%logger"/>
        </layout>
      </parameter>
      <parameter>
        <parameterName value="@dsc_mensaje"/>
        <dbType value="String"/>
        <size value="4000"/>
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%message"/>
        </layout>
      </parameter>
      <parameter>
        <parameterName value="@dsc_excepcion"/>
        <dbType value="String"/>
        <size value="2000"/>
        <layout type="log4net.Layout.ExceptionLayout"/>
      </parameter>
      <parameter>
        <parameterName value="@canal"/>
        <dbType value="string"/>
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%property{canal}"/>
        </layout>
      </parameter>
      <parameter>
        <parameterName value="@nmb_metodo"/>
        <dbType value="string"/>
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%property{nmb_metodo}"/>
        </layout>
      </parameter>
      <parameter>
        <parameterName value="@cdg_referencia"/>
        <dbType value="string"/>
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%property{cdg_referencia}"/>
        </layout>
      </parameter>
      <parameter>
        <parameterName value="@dsc_json_request"/>
        <dbType value="string"/>
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%property{dsc_json_request}"/>
        </layout>
      </parameter>
    </appender>
  </log4net>
